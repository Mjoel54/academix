{
  "name": "academix",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon ./src/server.ts",
    "start": "node ./build/server.js",
    "build": "npm install && tsc -p .",
    "seed": "node ./build/seeds/seed.js",
    "serve-dev": "nodemon ./src/server.ts",
    "test": "cross-env MONGO_URI=mongodb://localhost:27017/academix-test jest --collectCoverage --forceExit --detectOpenHandles"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http": "^0.0.1-security",
    "mongodb": "^6.1.0",
    "mongoose": "^7.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.19",
    "@types/jest": "^29.5.5",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.8.5",
    "@types/supertest": "^2.0.14",
    "@types/jsonwebtoken": "^9.0.6",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1"
  }
}
